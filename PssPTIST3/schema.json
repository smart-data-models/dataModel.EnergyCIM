        },
        {
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "PssPTIST3"
                    ],
                    "description": "Property. NGSI type. It has to be PssPTIST3"
                },
                "a0": {
                    "description": "Property. Model:'https://schema.org/Number'. Filter coefficient (A0). Default: 0.0",
